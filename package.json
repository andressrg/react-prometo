    "release": "git push origin HEAD && git push --tags && npm publish",
    "release:beta": "git push origin HEAD && git push --tags && npm publish --tag beta"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "microbundle": "^0.15.1",
    "react": "^18.2.0",
    "typescript": "^4.7.4"
