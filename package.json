{
  "name": "react-prometo",
  "version": "1.0.0",
  "description": "Easier promises in react",
  "source": "src/index.tsx",
  "type": "module",
  "exports": {
    "require": "./dist/react-prometo.cjs",
    "default": "./dist/react-prometo.modern.js"
  },
  "main": "dist/react-prometo.cjs",
  "module": "dist/react-prometo.module.js",
  "unpkg": "dist/react-prometo.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "repository": "https://github.com/andressrg/react-prometo.git",
  "author": "Andres Serrano <andres.srg@gmail.com>",
  "license": "MIT",
  "scripts": {
